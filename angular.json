{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "cli": {
    "analytics": false,
    "packageManager": "yarn",
    "schematicCollections": ["@cypress/schematic", "@schematics/angular"]
  },
  "projects": {
    "visualization-component": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "rxjs",
              "mathjs",
              "fast-equals",
              "html2canvas",
              "file-saver",
              "marked",
              "mousetrap",
              "lodash",
              "pan-zoom",
              "clone-deep",
              "@khiops-hypertree/js/d3-hypertree",
              "@khiops-treeview/treeview",
              "typed-function",
              "seedrandom",
              "javascript-natural-sort",
              "complex.js",
              "fraction.js",
              "escape-latex"
            ],
            "outputPath": "dist/visualization-component",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "./libs/khiops-treeview/treeview.scss",
              "./libs/khiops-hypertree/dist/d3-hypertree-light.css",
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "6mb",
                  "maximumError": "13mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "visualization-component:build:production"
            },
            "development": {
              "buildTarget": "visualization-component:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "buildTarget": "visualization-component:build"
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        },
        "cypress-run": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "visualization-component:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "visualization-component:serve:production"
            }
          }
        },
        "cypress-open": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "watch": true,
            "headless": false
          }
        },
        "ct": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "visualization-component:serve",
            "watch": true,
            "headless": false,
            "testingType": "component"
          },
          "configurations": {
            "development": {
              "devServerTarget": "visualization-component:serve:development"
            }
          }
        },
        "e2e": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "visualization-component:serve",
            "watch": true,
            "headless": false
          },
          "configurations": {
            "production": {
              "devServerTarget": "visualization-component:serve:production"
            }
          }
        }
      }
    },
    "khiops-webcomponent": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/khiops-webcomponent",
      "sourceRoot": "projects/khiops-webcomponent/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "rxjs",
              "mathjs",
              "fast-equals",
              "html2canvas",
              "file-saver",
              "marked",
              "mousetrap",
              "lodash",
              "pan-zoom",
              "clone-deep",
              "@khiops-hypertree/js/d3-hypertree",
              "@khiops-treeview",
              "typed-function",
              "seedrandom",
              "javascript-natural-sort",
              "complex.js",
              "fraction.js",
              "escape-latex"
            ],
            "outputPath": "dist/khiops-webcomponent",
            "index": "projects/khiops-webcomponent/src/index.html",
            "main": "projects/khiops-webcomponent/src/main.ts",
            "polyfills": "projects/khiops-webcomponent/src/polyfills.ts",
            "tsConfig": "projects/khiops-webcomponent/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/khiops-webcomponent/src/assets",
              "projects/khiops-webcomponent/src/package.json",
              "projects/khiops-webcomponent/src/README.md",
              "projects/khiops-webcomponent/src/frame.html",
              "projects/khiops-webcomponent/src/iframe.html"
            ],
            "styles": ["projects/khiops-webcomponent/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "6mb",
                  "maximumError": "13mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/khiops-webcomponent/src/environments/environment.ts",
                  "with": "projects/khiops-webcomponent/src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "none"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "khiops-webcomponent:build:production"
            },
            "development": {
              "buildTarget": "khiops-webcomponent:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "buildTarget": "khiops-webcomponent:build"
          }
        },
        "cypress-run": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "khiops-webcomponent:serve",
            "configFile": "projects/khiops-webcomponent/cypress.config.js"
          },
          "configurations": {
            "production": {
              "devServerTarget": "khiops-webcomponent:serve:production"
            }
          }
        },
        "cypress-open": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "watch": true,
            "headless": false,
            "configFile": "projects/khiops-webcomponent/cypress.config.js"
          }
        },
        "ct": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "khiops-webcomponent:serve",
            "watch": true,
            "headless": false,
            "testingType": "component"
          },
          "configurations": {
            "development": {
              "devServerTarget": "khiops-webcomponent:serve:development"
            }
          }
        },
        "e2e": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "khiops-webcomponent:serve",
            "watch": true,
            "headless": false
          },
          "configurations": {
            "production": {
              "devServerTarget": "khiops-webcomponent:serve:production"
            }
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
