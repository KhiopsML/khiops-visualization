    "@types/hammerjs": "2.0.39",
    "@types/jasmine": "3.8.0",
    "@types/lodash": "4.14.191",
    "@types/mousetrap": "1.6.3",
    "@types/node": "^20.9.0",
    "cypress": "latest",
    "jasmine-core": "5.1.1",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
