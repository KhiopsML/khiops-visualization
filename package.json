    "@angular/forms": "20.3.12",
    "@angular/material": "20.2.13",
    "@angular/platform-browser": "20.3.12",
    "@angular/platform-browser-dynamic": "20.3.12",
    "@angular/router": "20.3.12",
    "@ngrx/effects": "20.1.0",
    "@ngrx/store": "20.1.0",
    "@ngstack/translate": "10.0.0",
    "@webcomponents/webcomponentsjs": "2.8.0",
    "angular-resizable-element": "8.0.0",
    "angular-resize-event-package": "^3.7.0",
    "angular-split": "20.0.0",
    "angular2-hotkeys": "16.0.1",
    "chart.js": "4.5.1",
    "d3": "7.9.0",
    "document-register-element": "1.14.10",
    "fast-equals": "5.3.3",
    "file-saver": "2.0.5",
    "html2canvas": "1.4.1",
    "lodash-es": "4.17.21",
    "material-design-icons": "3.0.1",
    "pan-zoom": "3.5.2",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.3.10",
    "@angular/build": "^20.3.10",
    "@angular/cli": "20.3.10",
    "@angular/compiler-cli": "20.3.12",
    "@cypress/schematic": "4.1.4",
    "@types/cypress": "1.1.6",
    "@types/d3": "7.4.3",
    "@types/hammerjs": "2.0.46",
    "@types/jasmine": "5.1.13",
    "@types/lodash-es": "4.17.12",
    "@types/mousetrap": "1.6.15",
    "@types/node": "24.10.1",
    "autoprefixer": "10.4.22",
    "cypress": "15.6.0",
    "jasmine-core": "5.12.1",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ng-packagr": "20.3.2",
    "ngx-build-plus": "20.0.0",
    "postcss-loader": "8.2.0",
    "prettier": "3.6.2",
    "source-map-explorer": "2.5.3",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "webpack-bundle-analyzer": "5.0.0"
  },
  "resolutions": {
    "on-headers": "1.1.0"
  },
  "appdescription": {
    "visualization": "Khiops Visualization",
    "covisualization": "Khiops Covisualization"
  },
  "title": {
    "visualization": "Khiops Visualization",
    "covisualization": "Khiops Covisualization"
  }
}
