{
  "name": "visualization-component",
  "version": "11.0.7",
  "private": true,
  "author": {
    "name": "Orange",
    "email": "khiops@orange.com"
  },
  "browser": {
    "child_process": false,
    "electron": false,
    "http": false,
    "https": false,
    "net": false,
    "crypto": false,
    "path": false,
    "stream": false,
    "tls": false
  },
  "scripts": {
    "build": "ng build",
    "build:stats": "ng build --stats-json --source-map --output-hashing none && npm run stats:webpack-bundle-analyzer & npm run stats:source-map-explorer",
    "build:webcomponents": "./build.sh",
    "quickbuild:webcomponents": "./quickbuild.sh",
    "stats:webpack-bundle-analyzer": "webpack-bundle-analyzer dist/visualization-component/stats.json",
    "stats:source-map-explorer": "source-map-explorer dist/visualization-component/main.js --no-border-checks",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "ng": "ng",
    "start": "ng serve",
    "start:logs": "yarn run start > yarn-start.log 2>&1",
    "start:webcomponent": "ng serve khiops-webcomponent",
    "test": "ng test --watch=false",
    "test:watch": "ng test --watch=true",
    "watch": "ng build --watch --configuration development",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "pretty": "prettier --write \"./**/*.{ts,html, scss}\""
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "dependencies": {
    "@ag-grid-community/angular": "28.2.1",
